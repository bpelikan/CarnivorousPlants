
@{
    ViewData["Title"] = "Recognize";
}

<h1>Recognize</h1>

<form asp-controller="Plants" asp-action="Recognize" method="post" enctype="multipart/form-data">
    <div asp-validation-summary="All" class="text-danger"></div>
    <br />

    <input type="file" name="photo" />
    <hr />
    <input class="btn btn-primary" type="submit" value="Send" asp-route-returnUrl="@ViewData["ReturnUrl"]" />
    <a condition="@ViewData["ReturnUrl"] == null" class="btn btn-sm btn-default" asp-controller="Home" asp-action="Index">Back</a>
</form>

